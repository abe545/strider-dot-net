{
  "name": "strider-dot-net",
  "version": "0.6.0",
  "description": "build .NET projects",
  "homepage": "https://github.com/abe545/strider-dot-net",
  "bugs": "https://github.com/abe545/strider-dot-net/issues",
  "main": "worker.js",
  "dependencies": {
